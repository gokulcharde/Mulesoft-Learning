		<ee:transform doc:name="Transform Message" doc:id="9dee5f14-7cb3-49a1-913c-853e80ddca99" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
{
  "Name":attributes.queryParams.Name,
  "EmployeeId":attributes.queryParams.EmpId
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<logger level="INFO" doc:name="Logger" doc:id="da6630c4-16a5-4e60-ae0f-9e9e29c592d9" />
